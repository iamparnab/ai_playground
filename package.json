{
  "name": "ai_playground",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@monaco-editor/react": "^3.7.0",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.1.2",
    "@testing-library/user-event": "^12.2.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-scripts": "4.0.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "BROWSER=None react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy:clear": "aws s3 rm s3://ai-playground-campk12",
    "deploy:copy": "aws s3 cp build s3://ai-playground-campk12 --recursive --acl public-read",
    "deploy": "npm run deploy:clear && npm run deploy:copy && echo 'Go to http://ai-playground-campk12.s3-website.us-east-2.amazonaws.com'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "^14.14.7",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-redux": "^7.1.11",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "typescript": "^4.0.5"
  }
}
